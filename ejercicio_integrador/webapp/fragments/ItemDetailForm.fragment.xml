<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormDisplay354"
			editable="false"
			layout="ResponsiveGridLayout"
			title="{i18n>ORDER_DETAIL}"
			labelSpanXL="3"
			labelSpanL="3"
			labelSpanM="3"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			emptySpanS="0"
			columnsXL="1"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
			<f:content>
				<Label text="{i18n>ORDER_ID}" />
				<Text id="OrderIDText" text="{OrderID}" />
				<Label text="{i18n>PRODUCT_ID}" />
				<Text text="{test>/ProductID}" />
				<Label text="{i18n>PRICE}" />
				<Text text="{Price}" />
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>